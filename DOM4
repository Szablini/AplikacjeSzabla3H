    <script>
        function czas(){
            var data = new Date();
            var godzina = data.getHours();
            var minuta = data.getMinutes();
            var sekunda = data.getSeconds();

            document.title = godzina + "-" + minuta+"-"+sekunda;
            
        }
        const tytuÅ‚ = setInterval(czas, 0);
    </script>
